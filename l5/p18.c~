#include <stdio.h>
#include <stdint.h>

void print_bits(uint32_t n){
  uint32_t mask=1<<31;

  for(int i=0 ; i<32 ; i++){
    if(n&mask)
      printf("1");
    else
      printf("0");

    mask>>=1;
  }

  return ;
}

uint32_t f(uint32_t nr, uint8_t n, int p){
  for(int i=0 ; i<8 ; i++){
    if(n&1){
      nr=nr | (1<<(p+i));
    }else{
      nr=nr & (~(1<<(p+i)));
    }

    n>>=1;
  }

  return nr;
}

