#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char **lines=NULL;
int idxLin=0;

int main(){
  char *line=NULL;
  size_t size=0;
  
  if((fin=fopen("scrisoare.txt","wt"))==NULL){
    return 1;
  }

  while((getline(&line,&size,fin))>0){
    strcpy(lines[idxLin],line);
    idxLin++;
  }

  if((fout=fopen("outt.txt","wt"))==NULL){
    return 1;
  }
  
  for(int i=0 ; i<idxLin ; i++){
    fprintf(fout,"%s\n",lines[i]);
  }

  fclose(fin);
  fclose(fout);
  free(line);
  for(int i=0 ; i<idxLin ; i++){
    free(lines[i]);
  }

  free(lines);
  
  return 0;
}
