#include <stdio.h>
#include <stdlib.h>
#include <time.h>


int main(int argc, char **argv){
  srand(time(NULL));
  char chars[]="qwertyuiopassdfghjklzxcvbnmQWERTYUIOPASDFGHJKLZXCVBNM1234567890";

  int dim=atoi(argc[1]);

  FILE *fout=NULL;

  if(!(fout=fopen(argc[2],"wt"))){
    printf("err fisier");
    return 1;
  }

  char *s=malloc(dim+1);
  for(int i=0 ; i<dim ; i++){
    char c=chars[rand()%strlen(chars)];
    s[i]=c;
  }

  s[size]=0;
  fprintf(fout,"%s",s);

  free(s);
  fclose(fout);
  
  return 0;
}
